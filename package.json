{
  "name": "heura",
  "version": "1.0.0",
  "description": "Syntax highlighting for Heura files.",
  "author": "ferranbt",
  "publisher": "ferranbt",
  "engines": {
    "vscode": "^1.20.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "heura",
        "aliases": [
          "Heura",
          "heura"
        ],
        "extensions": [
          ".hra"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "heura",
        "scopeName": "source.heura",
        "path": "./syntaxes/heura.tmlanguage.json"
      }
    ]
  }
}
